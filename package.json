{
  "name": "eui-component-template",
  "version": "0.1.0",
  "description": "EUI component template",
  "main": "dist/index.js",
  "scripts": {
    "start": "eui server",
    "dev": "eui server",
    "build": "eui build",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint ./src & eslint ./demo & eslint ./__test__ & eslint ./__mocks__",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "author": "mistong eui team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MST-EUI/EUI-component-tpl.git"
  },
  "bugs": {
    "url": "https://github.com/MST-EUI/EUI-component-tpl/issues"
  },
  "homepage": "https://github.com/MST-EUI/EUI-component-tpl",
  "keywords": [
    "react",
    "react-component",
    "eui"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mistong/eui-cli": "^0.1.6",
    "@mistong/eui-css": "^0.1.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.2.0",
    "babel-loader": "^6.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "css-loader": "^0.26.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-14": "^1.0.5",
    "es3ify-loader": "^0.2.0",
    "es5-shim": "^4.5.10",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^0.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "jest": "^23.2.0",
    "node-sass": "^4.8.3",
    "react": "^0.14.9",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.9",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.13.1",
    "webpack-hot-middleware": "^2.22.2"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  }
}
